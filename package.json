{
  "name": "@sjofartstidningen/nanny",
  "version": "0.0.0",
  "description": "An image processing service compatible with humanmade/tachyon",
  "main": "index.js",
  "repository": "git@github.com:sjofartstidningen/nanny.git",
  "author": "Adam Bergman <adam@fransvilhelm.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "ncc build src/handler.ts -o dist --external aws-sdk",
    "test": "jest",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "deploy": "lambi --env-file .env --env STAGE=${STAGE} 'yarn run build && serverless deploy'",
    "deploy:fn": "lambi --env-file .env --env STAGE=${STAGE} 'yarn run build && serverless deploy -f processImage'",
    "remove": "lambi --env-file .env --env STAGE=${STAGE} 'serverless remove --stage=${STAGE}'",
    "docker:run": "docker run --env-file=.env nanny /bin/bash -c",
    "predocker:run": "docker build -t nanny .",
    "example": "parcel serve preview/index.pug",
    "prepare": "husky install"
  },
  "dependencies": {
    "animated-gif-detector": "^1.2.0",
    "aws-sdk": "^2.1248.0",
    "http-errors": "^2.0.0",
    "mime-types": "^2.1.35",
    "ramda": "^0.28.0",
    "sharp": "^0.31.2",
    "smartcrop-sharp": "^2.0.6",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/aws-lambda": "^8.10.108",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.2.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.9",
    "@types/ramda": "^0.28.18",
    "@types/s3rver": "^3.7.0",
    "@types/sharp": "^0.31.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.27.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "serverless": "^2.72.3",
    "serverless-api-cloudfront": "^0.9.5",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix"
    ],
    "*.{js,ts,jsx,tsx,json,md,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "printWidth": 120,
    "importOrder": [
      "^node:(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^~(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "volta": {
    "node": "16.18.1"
  }
}
